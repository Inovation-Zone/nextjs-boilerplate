{
  "name": "nextjs-structure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "open http://localhost:3000 && next start",
    "start-test": "REACT_APP_NODE_ENV=test && open http://localhost:3000 && next dev",
    "start-prod": "REACT_APP_NODE_ENV=production && open http://localhost:3000 && next dev",
    "build": "REACT_APP_NODE_ENV=production && next build",
    "lint": "next lint",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "build:analyze": "ANALYZE=true npm run build",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@reduxjs/toolkit": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "axios": "^0.21.1",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "next": "12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "sass": "^1.32.5"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^9.5.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "postcss": "^8.2.4",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
